<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name="configure.test.configurebuilds.StartUp"
        android:label="@string/app_name"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name="configure.test.configurebuilds.activities.MiscellaneousListActivity"
            android:label="@string/title_activity_miscellaneous_list"
            android:parentActivityName="configure.test.configurebuilds.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            tools:ignore="GoogleAppIndexingWarning">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="configure.test.configurebuilds.MainActivity" />
        </activity>

        <activity
            android:name="configure.test.configurebuilds.activities.test101.SmsRetrieverTest101Activity"
            android:label="SMS Retriever"
            android:parentActivityName="configure.test.configurebuilds.activities.MiscellaneousListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="configure.test.configurebuilds.activities.MiscellaneousListActivity" />
        </activity>

        <receiver
            android:name="configure.test.configurebuilds.activities.test101.MySMSBroadcastReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.android.gms.auth.api.phone.SMS_RETRIEVED"/>
            </intent-filter>
        </receiver>


    </application>

</manifest>